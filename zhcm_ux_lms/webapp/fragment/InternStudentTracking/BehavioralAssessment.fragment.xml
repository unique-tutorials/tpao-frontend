<c:FragmentDefinition xmlns="sap.m"
    xmlns:c="sap.ui.core">
 
    <VBox class="sapUiSmallMarginTopBottom">
        <MessageStrip class="sapUiSmallMarginBottom" text="Dolduracağınız bu anketin amacı Biz Bize Kariyer Programı Kapsamında gelişimine destek verdiğiniz
        stajerlerinizi değerlendirmek ve Biz Bize Kariyer programını geliştir olup, dolduracağınız anket bizim için
        önem arz etmektedir." type="Information" showIcon="true">
        </MessageStrip>
    </VBox>
    <IconTabBar stretchContentHeight="false" id="TabContainer2">
        <items>
            <IconTabFilter text="Lise">
                <Table id="idProductsTable2" inset="false" items="{/InternEvaluationFormSet}">
                    <headerToolbar>
                        <OverflowToolbar>
                            <content>
                                <Title text="Ölçeklere Göre Puan Skalası" level="H2"/>
                                <ToolbarSpacer />
                            </content>
                        </OverflowToolbar>
                    </headerToolbar>
                    <columns>
                        <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="{i18n>Q_ORDER_NUMBER}" />
                        </Column>
                        <!-- <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="sorular" />
                        </Column> -->
                        <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="{i18n>I_FINALLY_AGREE}" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="{i18n>I_AGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>PARTIALLY_AGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>I_M_UNDECIDED}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>SOMEWHAT_DISAGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>I_DISAGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>I_STRONGLY_DISAGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>NO_IDEA}" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem vAlign="Middle">
                            <cells>
                                <Text text="{Quenm}" />
                                <!-- <Text text="{Quetx}" /> -->
                                <Text text="{Score1}" />
                                <Text text="{Score2}" />
                                <Text text="{Score3}" />
                                <Text text="{Score4}" />
                                <Text text="{Score5}" />
                                <Text text="{Score6}" />
                                <Text text="{Score7}" />
                                <Text text="{Score8}" />
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>

                <Table inset="false" items="{/EvaluationAnswersSet}" growing="true" growingScrollToLoad="true">
                    <headerToolbar>
                        <OverflowToolbar>
                            <content>
                                <Title text="Lise Öğrencileri İçin Değerlendirme Anketi" level="H2"/>
                                <!-- <ToolbarSpacer />
                                <Button icon="sap-icon://edit" press="onEditPress" type="Accept" /> -->
                            </content>
                        </OverflowToolbar>
                    </headerToolbar>
                    <columns>
                        <Column width="8rem">
                            <Text text="{i18n>STUDENT_NUMBER}" />
                        </Column>
                        <Column width="8rem">
                            <Text text="{i18n>Q_ORDER_NUMBER}" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true" width="25rem">
                            <Text text="{i18n>EVALUATION_QUESTIONS}" />
                        </Column>
                        <Column>
                            <Text text="{i18n>I_FINALLY_AGREE}" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="{i18n>I_AGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>PARTIALLY_AGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>I_M_UNDECIDED}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>SOMEWHAT_DISAGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>I_DISAGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>I_STRONGLY_DISAGREE}" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="{i18n>NO_IDEA}" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem vAlign="Middle">
                            <cells>
                                <Text text="{Pernr}"/>
                                <Text text="{Quenm}"/>
                                <Text text="{Quetx}"/>
                                <Text text="{Answe}"/>
                                <Text text="{Answe}"/>
                                <Text text="{Answe}"/>
                                <Text text="{Answe}"/>
                                <Text text="{Answe}"/>
                                <Text text="{Answe}"/>
                                <Text text="{Answe}"/>
                                <Text text="{Answe}"/>
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
            </IconTabFilter>
            <IconTabFilter text="Üniversite">
            </IconTabFilter>
        </items>
    </IconTabBar>

</c:FragmentDefinition>