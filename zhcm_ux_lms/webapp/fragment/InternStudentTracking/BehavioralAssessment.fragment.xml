<c:FragmentDefinition xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <f:Form editable="true">
        <f:title>
            <core:Title text="{i18n>TECHNICAL_EVALUATION}" />
        </f:title>
        <f:layout>
            <f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="1" singleContainerFullSize="false" />
        </f:layout>
        <f:formContainers>
            <f:FormContainer>
                <f:formElements>
                    <f:FormElement label="{i18n>Q_UNIT}">
                        <f:fields>
                            <Input value="{SupplierName}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>UNIT}">
                        <f:fields>
                            <Input value="{Street}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>STUDENT_ENAME}">
                        <f:fields>
                            <Input value="{ZIPCode}" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>
            <f:FormContainer>
                <f:formElements>
                    <f:FormElement label="{i18n>EVALUATION_DATE}">
                        <f:fields>
                            <Input value="{ZIPCode}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>TECHNICAL_ENAME}">
                        <f:fields>
                            <Input value="{Street}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>REGISTRATION_NUMBER}">
                        <f:fields>
                            <Input value="{ZIPCode}" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>
        </f:formContainers>
    </f:Form>
    <VBox class="sapUiSmallMarginTopBottom">
        <MessageStrip class="sapUiSmallMarginBottom" text="Dolduracağınız bu anketin amacı Biz Bize Kariyer Programı Kapsamında gelişimine destek verdiğiniz
        stajerlerinizi değerlendirmek ve Biz Bize Kariyer programını geliştir olup, dolduracağınız anket bizim için
        önem arz etmektedir." type="Information" showIcon="true">
        </MessageStrip>
    </VBox>
    <IconTabBar stretchContentHeight="false" id="TabContainer2">
        <items>
            <IconTabFilter text="Lise">
                <!-- <List headerText="{i18n>INFO_LEVEL_DESC}" headerLevel="H2" items="{internStudentListModel>/ratingScaleList}">
                    <StandardListItem title="{internStudentListModel>titles}" />
                </List> -->
                <Table id="idProductsTable2" inset="false" items="{/InternEvaluationFormSet}">
                    <headerToolbar>
                        <OverflowToolbar>
                            <content>
                                <Title text="Ölçeklere Göre Puan Skalası" level="H2"/>
                                <ToolbarSpacer />
                            </content>
                        </OverflowToolbar>
                    </headerToolbar>
                    <columns>
                        <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="Soru Sıra No" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="Kesinlikle Katılıyorum" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="Katılıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Kısmen Katılıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Kararsızım" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Kısmen Katılmıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Katılmıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Kesinlikle Katılmıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Fikrim Yok" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem vAlign="Middle">
                            <cells>
                                <Text text="{Quenm}" />
                                <Text text="{Score1}" />
                                <Text text="{Score2}" />
                                <Text text="{Score3}" />
                                <Text text="{Score4}" />
                                <Text text="{Score5}" />
                                <Text text="{Score6}" />
                                <Text text="{Score7}" />
                                <Text text="{Score8}" />
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>

                <Table inset="false" items="{internStudentListModel>/evaluationQuestionsList}" growing="true" growingScrollToLoad="true">
                    <headerToolbar>
                        <OverflowToolbar>
                            <content>
                                <Title text="Lise Öğrencileri İçin Değerlendirme Anketi" level="H2"/>
                                <ToolbarSpacer />
                                <Button icon="sap-icon://edit" press="onEditPress" type="Accept" />
                            </content>
                        </OverflowToolbar>
                    </headerToolbar>
                    <columns>
                        <Column width="8rem">
                            <Text text="{i18n>Q_ORDER_NUMBER}" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true" width="30rem">
                            <Text text="{i18n>EVALUATION_QUESTIONS}" />
                        </Column>
                        <Column>
                            <Text text="Kesinlikle Katılıyorum" />
                        </Column>
                        <Column minScreenWidth="Tablet" demandPopin="true">
                            <Text text="Katılıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Kısmen Katılıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Kararsızım" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Kısmen Katılmıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Katılmıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Kesinlikle Katılmıyorum" />
                        </Column>
                        <Column minScreenWidth="Desktop" demandPopin="true">
                            <Text text="Fikrim Yok" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem vAlign="Middle">
                            <cells>
                                <Text text="{internStudentListModel>numb}"/>
                                <Text text="{internStudentListModel>Ques}" />
                                <!-- <HBox alignItems="End" justifyContent="End">
                                    <Input value="{internStudentListModel>Point}" visible="{= ${internStudentListModel>/suggestionActionData/priorityEditable} }" />
                                    <Text text="{internStudentListModel>Point}" visible="{= ${internStudentListModel>/suggestionActionData/priorityDisplay} }" />
                                </HBox> -->
                                <Text text="{internStudentListModel>numb}"/>
                                <Text text="{internStudentListModel>numb}"/>
                                <Text text="{internStudentListModel>numb}"/>
                                <Text text="{internStudentListModel>numb}"/>
                                <Text text="{internStudentListModel>numb}"/>
                                <Text text="{internStudentListModel>numb}"/>
                                <Text text="{internStudentListModel>numb}"/>
                                <Text text="{internStudentListModel>numb}"/>
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
            </IconTabFilter>
            <IconTabFilter text="Üniversite">
            </IconTabFilter>
        </items>
    </IconTabBar>

</c:FragmentDefinition>