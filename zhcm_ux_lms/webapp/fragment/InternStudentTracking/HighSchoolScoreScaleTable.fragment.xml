<c:FragmentDefinition xmlns="sap.m"
    xmlns:c="sap.ui.core">
    <Table id="idPointsHighTable" items="{/InternEvaluationFormSet}" inset="false" visible="{= ${internStudentListModel>/selectedInternEmployee/Ptext} === 'Lise Stajyeri' }">
        <headerToolbar>
            <OverflowToolbar>
                <content>
                    <Title text="{i18n>SCR_SCL_HS}" level="H2"/>
                    <ToolbarSpacer />
                </content>
            </OverflowToolbar>
        </headerToolbar>
        <columns>
            <Column>
                <Text text="{i18n>Q_ORDER_NUMBER}" />
            </Column>
            <Column>
                <Text text="{i18n>I_FINALLY_AGREE}" />
            </Column>
            <Column>
                <Text text="{i18n>I_AGREE}" />
            </Column>
            <Column minScreenWidth="Desktop" demandPopin="true">
                <Text text="{i18n>PARTIALLY_AGREE}" />
            </Column>
            <Column minScreenWidth="Desktop" demandPopin="true">
                <Text text="{i18n>I_M_UNDECIDED}" />
            </Column>
            <Column minScreenWidth="Desktop" demandPopin="true">
                <Text text="{i18n>SOMEWHAT_DISAGREE}" />
            </Column>
            <Column minScreenWidth="Desktop" demandPopin="true">
                <Text text="{i18n>I_DISAGREE}" />
            </Column>
            <Column minScreenWidth="Desktop" demandPopin="true">
                <Text text="{i18n>I_STRONGLY_DISAGREE}" />
            </Column>
            <Column minScreenWidth="Desktop" demandPopin="true">
                <Text text="{i18n>NO_IDEA}" />
            </Column>
        </columns>
        <items>
            <ColumnListItem vAlign="Middle">
                <cells>
                    <Text text="{Quenm}" />
                    <Text text="{Score1}" />
                    <Text text="{Score2}" />
                    <Text text="{Score3}" />
                    <Text text="{Score4}" />
                    <Text text="{Score5}" />
                    <Text text="{Score6}" />
                    <Text text="{Score7}" />
                    <Text text="{Score8}" />
                </cells>
            </ColumnListItem>
        </items>
    </Table>
</c:FragmentDefinition>