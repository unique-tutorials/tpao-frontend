<c:FragmentDefinition xmlns="sap.m"
    xmlns:c="sap.ui.core"
    xmlns:f="sap.ui.layout.form"
    xmlns:core="sap.ui.core">
    <f:Form editable="true">
        <f:title>
            <core:Title text="{i18n>STUDENT_ACCOUNT_INFORMATION}" />
        </f:title>
        <f:layout>
            <f:ResponsiveGridLayout labelSpanXL="4" labelSpanL="4" labelSpanM="12" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="0" emptySpanL="0" emptySpanM="0" emptySpanS="0" columnsXL="2" columnsL="2" columnsM="2" singleContainerFullSize="false" />
        </f:layout>
        <f:formContainers>
            <f:FormContainer title="{i18n>DOMESTIC}">
                <f:formElements>
                    <f:FormElement label="Partner SeÃ§iniz">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/domesticAccountEmployee/Partner}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>BANK_NAME}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/domesticEmployee/Banka}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>BANK_BRANCH}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/domesticEmployee/Brnch}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>BANK_CITY}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/domesticEmployee/City}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>TL_ACCOUNT_NUMBER}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/domesticEmployee/Bankn}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>TL_IBAN}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/domesticEmployee/Iban00}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>EXPLANATION}">
                        <f:fields>
                            <TextArea rows="3" value="{AbrAccountListModel>/domesticEmployee/Descp}" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>
            <f:FormContainer title="{i18n>ABROAD}">
                <f:formElements>
                    <f:FormElement label="{i18n>BANK_NAME}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherAccountEmployee/Partner}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>BANK_NAME}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherAccountEmployee/Banka}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>BANK_BRANCH}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherAccountEmployee/Brnch}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>BANK_CITY}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherAccountEmployee/City}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>ACCOUNT_NUMBER}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherAccountEmployee/Bankn}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>SWIFT_CODE}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherAccountEmployee/Swift}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>IBAN}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherAccountEmployee/Iban00}" />
                        </f:fields>
                    </f:FormElement>
                    <!-- <f:FormElement label="{i18n>ACC_NO}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherEmployee/Banka}" />
                        </f:fields>
                    </f:FormElement> -->
                    <f:FormElement label="{i18n>ABA_NO}">
                        <f:fields>
                            <Input value="{AbrAccountListModel>/abroadOtherAccountEmployee/Abano}" />
                        </f:fields>
                    </f:FormElement>
                    <f:FormElement label="{i18n>EXPLANATION}">
                        <f:fields>
                            <TextArea rows="3" value="{AbrAccountListModel>/abroadOtherAccountEmployee/Descp}" />
                        </f:fields>
                    </f:FormElement>
                </f:formElements>
            </f:FormContainer>
        </f:formContainers>
    </f:Form>
</c:FragmentDefinition>