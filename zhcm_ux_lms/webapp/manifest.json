				"CareerInterns": {
					"viewName": "CareerInterns",
					"viewId": "CareerInterns",
					"viewLevel": 2,
					"title": "Biz Bize Kariyer Stajyerler"
				},
				"TrainingManager": {
					"viewName": "TrainingManager",
					"viewId": "TrainingManager",
					"viewLevel": 2,
					"title": "Eğitim Yöneticisi"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/customLMS.css"
				}
			],
			"js": [
				{
					"uri": "utils/lodash.js"
