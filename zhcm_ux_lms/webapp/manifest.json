				"AbrTracking": {
					"viewName": "AbrTracking",
					"viewId": "AbrTracking",
					"viewLevel": 2,
					"title": "Burslu Öğrenci Takibi"
				},
				"AbrRequestList": {
					"viewName": "AbrRequestList",
