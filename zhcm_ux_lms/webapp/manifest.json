{
	"_version": "1.53.0",
	"sap.app": {
		"id": "zhcm_ux_lms_abr",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"resources": "resources.json",
		"sourceTemplate": {
			"id": "@sap/generator-fiori:basic",
			"version": "1.13.2",
			"toolsId": "82e08e57-1293-4dc9-8ee6-33d79267bb3e"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZHCM_UX_LMS_ABR_SRV/",
				"type": "OData",
				"settings": {
					"annotations": [],
					"localUri": "localService/metadata.xml",
					"odataVersion": "2.0"
				}
			},
			"ZHCM_GENERIC_SRV": {
				"uri": "/sap/opu/odata/sap/ZHCM_GENERIC_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/ZHCM_GENERIC_SRV/metadata.xml"
				}
			}
		},
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponentForManifest",
			"version": "0.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"dependencies": {
			"minUI5Version": "1.114.0",
			"libs": {
				"sap.m": {},
				"sap.ui.core": {},
				"sap.f": {},
				"sap.suite.ui.generic.template": {},
				"sap.ui.comp": {},
				"sap.ui.generic.app": {},
				"sap.ui.table": {},
				"sap.ushell": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "zhcm_ux_lms_abr.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true,
				"settings": {}
			},
			"GenericServices": {
				"uri": "/sap/opu/odata/sap/ZHCM_GENERIC_SRV/",
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneWay",
					"defaultCountMode": "None"
				},
				"dataSource": "ZHCM_GENERIC_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"services": {
			"ShellUIService": {
				"factoryName": "sap.ushell.ui5service.ShellUIService",
				"lazy": false,
				"settings": {
					"setTitle": "auto"
				}
			}
		},
		"routing": {
			"config": {
				"sample": {
					"stretch": true,
					"files": [
						"AbrStajyerTracking.view.xml",
						"InternStudentTracking.view.xml",
						"InternStudentTracking.controller.js",
						"AbrStajyerTracking.controller.js",
						"CareerInterns.view.xml",
						"CareerInterns.controller.js",
						"manifest.json"
					]
				},
				"fullWidth": true,
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "zhcm_ux_lms_abr.view",
				"controlAggregation": "pages",
				"controlId": "app",
				"clearControlAggregation": false
			},
			"routes": [
				{
					"pattern": "",
					"name": "appdispatcher",
					"target": [
						"appdispatcher"
					]
				},
				{
					"pattern": "AbrRequestList",
					"name": "AbrRequestList",
					"target": [
						"AbrRequestList"
					]
				},
				{
					"pattern": "AbrFileUpload",
					"name": "AbrFileUpload",
					"target": [
						"AbrFileUpload"
					]
				},
				{
					"pattern": "AbrAccountTracking",
					"name": "AbrAccountTracking",
					"target": [
						"AbrAccountTracking"
					]
				},
				{
					"pattern": "AbrTracking",
					"name": "AbrTracking",
					"target": [
						"AbrTracking"
					]
				},
				{
					"pattern": "ApprovalRequestList",
					"name": "ApprovalRequestList",
					"target": [
						"ApprovalRequestList"
					]
				},
				{
					"pattern": "TrplsApp",
					"name": "TrplsApp",
					"target": [
						"TrplsApp"
					]
				},
				{
					"pattern": "TrpolApp",
					"name": "TrpolApp",
					"target": [
						"TrpolApp"
					]
				},
				{
					"pattern": "CareerInterns",
					"name": "CareerInterns",
					"target": [
						"CareerInterns"
					]
				},
				{
					"pattern": "TrainingManager",
					"name": "TrainingManager",
					"target": [
						"TrainingManager"
					]
				}
			],
			"targets": {
				"appdispatcher": {
					"viewName": "AppDispatcher",
					"viewId": "appdispatcher",
					"viewLevel": 1,
					"title": "{i18n>LMS_APPLICATIONS}"
				},
				"AbrFileUpload": {
					"viewName": "AbrFileUpload",
					"viewId": "AbrFileUpload",
					"viewLevel": 2,
					"title": "Burslu Öğrenci Belgelerin Sisteme Yüklenmesi"
				},
				"AbrAccountTracking": {
					"viewName": "AbrAccountTracking",
