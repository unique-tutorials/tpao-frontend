					"uri": "utils/FileDownloadJquery.js"
				},
				{
					"uri": "utils/moment.js"
				},
				{
					"uri": "utils/XLSX.js"
				},
				{
					"uri": "utils/xlsx-bundle.js"
				}
			]
		},
		"rootView": {
			"viewName": "zhcm_ux_lms_abr.view.App",
